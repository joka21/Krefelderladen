{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "off", // Deaktiviert die Regel für ungenutzte Variablen
    "@typescript-eslint/no-explicit-any": "warn", // Gibt nur eine Warnung bei 'any' aus
    "react/no-unescaped-entities": "off", // Deaktiviert die Regel für unescaped entities in JSX
    "no-console": "warn" // Gibt eine Warnung für console.log
  }
}
